{
  "name": "starter-visual-recognition",
  "version": "0.0.1",
  "description": "A Visual Recognition sample nodejs application using deep learning algorithms to analyze images that can give you insights into your visual content.",
  "private": true,
  "engines": {
    "node": "^8.0"
  },
  "config": {
    "entrypoint": "server/server.js"
  },
  "scripts": {
    "start": "node server/server.js",
    "debug": "node --inspect=0.0.0.0:9229 $npm_package_config_entrypoint",
    "debug:brk": "node --inspect-brk=0.0.0.0:9229 $npm_package_config_entrypoint",
    "debug:legacy": "node --debug=0.0.0.0:5858 $npm_package_config_entrypoint",
    "test": "nyc mocha --exit",
    "dev": "nodemon $npm_package_config_entrypoint",
    "dev_react": "nf --procfile Procfile-dev --port 3000 start",
    "postinstall": "npm run build",
    "build": "webpack",
    "server-reload": "nodemon $npm_package_config_entrypoint"
  },
  "nodemonConfig": {
    "env": {
      "NODE_HEAPDUMP_OPTIONS": "nosignal"
    }
  },
  "dependencies": {
    "async": "^2.1.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^7.3.0",
    "body-parser": "^1.16.0",
    "capitalize": "^1.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extend": "^3.0.1",
    "foreman": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "ibm-cloud-env": "^0",
    "jpath-query": "^0.1.1",
    "log4js": "^4.0.2",
    "multer": "^1.2.1",
    "node-sass": "^4.13.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.18.2",
    "uglifyify": "^4.0.3",
    "url-loader": "^3.0.0",
    "uuid": "^3.0.1",
    "vcap_services": "^0.3.4",
    "watson-developer-cloud": "^3.18.0",
    "watson-react-components": "^0.6.10",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "chai": "^4.2.0",
    "mocha": "^6.0.0",
    "nyc": "^13.3.0"
  },
  "author": "IBM Corp.",
  "license": "Apache-2.0"
}
